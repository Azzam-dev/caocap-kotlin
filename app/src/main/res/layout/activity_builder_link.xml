<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/caocap_background_white"
        tools:context=".Buliders.BuilderLinkActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline17">

        <TextView
                android:id="@+id/builder_image_border"
                android:layout_width="105dp"
                android:layout_height="105dp"
                android:background="@drawable/edit_profile_red_color"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.513"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:srcCompat="@drawable/edit_profile_green_color" />

        <ImageView
                android:id="@+id/builder_link_select_caocap_image_button"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/edit_profile_caocap_dark_color"
                app:layout_constraintBottom_toBottomOf="@+id/builder_image_view"
                app:layout_constraintEnd_toEndOf="@+id/builder_image_view"
                app:layout_constraintStart_toStartOf="@+id/builder_image_view"
                app:layout_constraintTop_toTopOf="@+id/builder_image_view" />

        <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/builder_image_view"
                android:layout_width="96dp"
                android:layout_height="96dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4dp"

                android:scaleType="centerCrop"
                android:src="@drawable/caocap_icon"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/builder_image_border">

            <Button
                    android:id="@+id/builder_link_red_border_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="32dp"
                    android:background="@drawable/edit_profile_red_color"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/builder_link_orange_border_button"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <Button
                    android:id="@+id/builder_link_orange_border_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/edit_profile_orange_color"
                    app:layout_constraintBottom_toBottomOf="@+id/builder_link_red_border_button"
                    app:layout_constraintEnd_toStartOf="@+id/builder_link_green_border_button"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/builder_link_red_border_button"
                    app:layout_constraintTop_toTopOf="@+id/builder_link_red_border_button" />

            <Button
                    android:id="@+id/builder_link_green_border_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/edit_profile_green_color"
                    app:layout_constraintBottom_toBottomOf="@+id/builder_link_orange_border_button"
                    app:layout_constraintEnd_toStartOf="@+id/builder_link_blue_border_button"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/builder_link_orange_border_button"
                    app:layout_constraintTop_toTopOf="@+id/builder_link_orange_border_button" />

            <Button
                    android:id="@+id/builder_link_blue_border_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/edit_profile_blue_color"
                    app:layout_constraintBottom_toBottomOf="@+id/builder_link_green_border_button"
                    app:layout_constraintEnd_toStartOf="@+id/builder_link_pink_border_button"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/builder_link_green_border_button"
                    app:layout_constraintTop_toTopOf="@+id/builder_link_green_border_button" />

            <Button
                    android:id="@+id/builder_link_pink_border_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/edit_profile_pink_color"
                    app:layout_constraintBottom_toBottomOf="@+id/builder_link_blue_border_button"
                    app:layout_constraintEnd_toStartOf="@+id/builder_link_white_border_button"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/builder_link_blue_border_button"
                    app:layout_constraintTop_toTopOf="@+id/builder_link_blue_border_button" />

            <Button
                    android:id="@+id/builder_link_white_border_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginEnd="32dp"
                    android:background="@drawable/edit_profile_white_color"
                    app:layout_constraintBottom_toBottomOf="@+id/builder_link_pink_border_button"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/builder_link_pink_border_button"
                    app:layout_constraintTop_toTopOf="@+id/builder_link_pink_border_button" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <EditText
                android:id="@+id/builder_link_coacap_name_edit_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="32dp"
                android:background="@drawable/builder_link_edit_text_border"
                android:ems="10"
                android:hint="caocap name"
                android:inputType="textPersonName"
                android:padding="5dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout6" />

        <EditText
                android:id="@+id/builder_link_coacap_link_edit_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:background="@drawable/builder_link_edit_text_border"
                android:ems="10"
                android:hint="caocap link"
                android:inputType="textPersonName"
                android:padding="5dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="https://"
                app:layout_constraintEnd_toEndOf="@+id/builder_link_coacap_name_edit_text"
                app:layout_constraintStart_toStartOf="@+id/builder_link_coacap_name_edit_text"
                app:layout_constraintTop_toBottomOf="@+id/builder_link_coacap_name_edit_text" />

        <TextView
                android:id="@+id/builder_link_create_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:background="@drawable/builder_link_create_button"
                android:padding="4dp"
                android:text="create"
                android:textAlignment="center"
                android:textColor="@android:color/white"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="@+id/builder_link_coacap_link_edit_text"
                app:layout_constraintStart_toStartOf="@+id/builder_link_coacap_link_edit_text"
                app:layout_constraintTop_toBottomOf="@+id/builder_link_coacap_link_edit_text" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2" />

</androidx.constraintlayout.widget.ConstraintLayout>